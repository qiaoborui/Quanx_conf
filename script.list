#自用
[Script]
hostname = app.bilibili.com,api.m.jd.com,oauth.secure.pixiv.net,education.github.com
http-response https://app.bilibili.com/x/resource/show/tab\?access_key script-path=https://raw.githubusercontent.com/qiaoborui/Quanx_conf/master/script/Loon_script/bili_loon.js, requires-body=true, timeout=10, tag=BililiTab
cron "*/10 * * * *" script-path=https://raw.githubusercontent.com/qiaoborui/Quanx_conf/master/script/testflight.js
cron "5 0 * * *" script-path=https://raw.githubusercontent.com/qiaoborui/Quanx_conf/master/script/JD_DailyBonus.js, tag=JD_DailyBonus.js
# 获取京东Cookie.
http-request https:\/\/api\.m\.jd\.com\/client\.action.*functionId=signBean script-path=https://raw.githubusercontent.com/qiaoborui/Quanx_conf/master/script/JD_DailyBonus.js, timeout=10, enable=false, tag=JD_DailyBonus.js
#pixiv
http-response https://oauth.secure.pixiv.net/auth/token script-path=https://raw.githubusercontent.com/qiaoborui/Quanx_conf/master/script/pixiv.js, requires-body=true, timeout=10, tag=pixiv
#workingcopy
http-response ^https:\/\/education\.github\.com\/api\/user$ requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/qiaoborui/Quanx_conf/master/script/Loon_script/workingcopy.js,script-update-interval=0
#sliver
cron "45 1 0 * * *" script-path=https://raw.githubusercontent.com/qiaoborui/Quanx_conf/master/script/Loon_script/slivertocoin.js,tag=瓜子转硬币